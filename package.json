{
	"name": "mern-boilerplate",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev:server": "rm -rf ./dist && tsc -outDir dist ./server/index.ts && nodemon --exec babel-node ./dist/index.js",
		"build:client": "rm -rf ./client/build && webpack --config webpack.prod.js",
		"build:server": "rm -rf ./dist && webpack --mode production --config webpack.server.js",
		"start": "node ./dist",
		"dev:client": "webpack-dev-server --config webpack.dev.js",
		"analyse": "webpack --config webpack.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
		"dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
		"build": "npm run build:server && npm run build:client"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-do-expressions": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@hot-loader/react-dom": "^16.8.6",
		"@types/cors": "^2.8.5",
		"@types/express": "^4.17.0",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.4",
		"@types/react-router-dom": "^4.3.4",
		"babel-loader": "^8.0.6",
		"compression-webpack-plugin": "^3.0.0",
		"concurrently": "^4.1.0",
		"css-loader": "^3.0.0",
		"file-loader": "^4.0.0",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.7.0",
		"nodemon": "^1.19.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"react-hot-loader": "^4.11.1",
		"sass": "^1.22.5",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"terser-webpack-plugin": "^1.3.0",
		"typescript": "^3.5.3",
		"uglifyjs-webpack-plugin": "^2.1.3",
		"url-loader": "^2.0.0",
		"webpack": "^4.32.2",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.7.2",
		"webpack-manifest-plugin": "^2.0.4",
		"webpack-node-externals": "^1.7.2",
		"webpack-pwa-manifest": "^4.0.0",
		"workbox-webpack-plugin": "^4.3.1"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.0.1"
	}
}
